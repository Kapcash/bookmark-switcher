{
  "manifest_version": 2,
  "name": "Bookmark-switcher",
  "version": "1.0.0",
  "homepage_url": "https://github.com/Kapcash",
  "description": "Switch between several bookmark bars in a click!",
  "author": "Florent Catiau-Tristant",
  "default_locale": "en",
  "browser_specific_settings": {
    "gecko": {
      "id": "browserswitcher@addon.dev",
      "strict_min_version": "42.0"
    }
  },
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Switch bookmark bar",
    "default_icon": {
      "16": "icons/16.png",
      "48": "icons/48.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Space"
      },
      "description": "Switch bookmark toolbars"
    }
  }
}
